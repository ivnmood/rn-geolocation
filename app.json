    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.ivan.project_for_nibl",
      "versionCode": 1,
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION" ]
    }
  }
}
